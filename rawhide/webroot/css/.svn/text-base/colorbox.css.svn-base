    #cboxContent{background:#fff;}
        #cboxLoadedContent{margin-bottom:5px;}
        #cboxLoadingOverlay{background:url(http://content.bmwusa.com/rawhide_homepage/images/loading_background.png) no-repeat center center;}
        #cboxLoadingGraphic{background:url(http://content.bmwusa.com/rawhide_homepage/images/loading.gif) no-repeat center center;}
        #cboxTitle{position:absolute; bottom:-25px; left:0; text-align:center; width:100%; font-weight:bold; color:#7C7C7C;}
        #cboxCurrent{position:absolute; bottom:-25px; left:58px; font-weight:bold; color:#7C7C7C;}
        #cboxClose{
		position:absolute;
		top:3px;
		right:3px;
		background-image:url(http://content.bmwusa.com/rawhide_homepage/images/close_btn.png);
		background-repeat:no-repeat;
		background-position:right top; 
		width:30px; height:30px;
		display:block;
		text-indent:-9999px;
		z-index:9999;
		}
        #cboxPrevious, #cboxNext, #cboxSlideshow{position:absolute; /*bottom:-29px;*/ background:url(http://content.bmwusa.com/rawhide_homepage/images/controls.png) no-repeat 0px 0px; width:23px; height:23px; text-indent:-9999px;top:0px;}
        #cboxPrevious{left:0px; background-position: -51px -25px;}
        #cboxPrevious.hover{background-position:-51px 0px;}
        #cboxNext{left:27px; background-position:-75px -25px;}
        #cboxNext.hover{background-position:-75px 0px;}
        #cboxClose{background-position:0px 0px;}
        #cboxClose.hover{background-position:0px 0px;}
        
        .cboxSlideshow_on #cboxSlideshow{background-position:-125px 0px; right:27px;}
        .cboxSlideshow_on #cboxSlideshow.hover{background-position:-150px 0px;}
        .cboxSlideshow_off #cboxSlideshow{background-position:-150px -25px; right:27px;}
        .cboxSlideshow_off #cboxSlideshow.hover{background-position:-125px 0px;}
